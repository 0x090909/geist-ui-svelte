{
	"name": "geist-ui-svelte",
	"version": "0.0.12",
	"author": {
		"name": "Aidan Bleser",
		"url": "https://aidanbleser.com"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ieedan/geist-ui-svelte"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ieedan/geist-ui-svelte/issues"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./Button.svelte": {
			"types": "./dist/button/Button.svelte.d.ts",
			"svelte": "./dist/button/Button.svelte"
		},
		"./Toggle.svelte": {
			"types": "./dist/toggle/Toggle.svelte.d.ts",
			"svelte": "./dist/toggle/Toggle.svelte"
		},
		"./Card.svelte": {
			"types": "./dist/card/Card.svelte.d.ts",
			"svelte": "./dist/card/Card.svelte"
		},
		"./Spacer.svelte": {
			"types": "./dist/spacer/Spacer.svelte.d.ts",
			"svelte": "./dist/spacer/Spacer.svelte"
		},
		"./Divider.svelte": {
			"types": "./dist/divider/Divider.svelte.d.ts",
			"svelte": "./dist/divider/Divider.svelte"
		},
		"./FieldSet.svelte": {
			"types": "./dist/fieldset/FieldSet.svelte.d.ts",
			"svelte": "./dist/fieldset/FieldSet.svelte"
		},
		"./Text.svelte": {
			"types": "./dist/text/Text.svelte.d.ts",
			"svelte": "./dist/text/Text.svelte"
		},
		"./Key.svelte": {
			"types": "./dist/key/Key.svelte.d.ts",
			"svelte": "./dist/key/Key.svelte"
		},
		"./Input.svelte": {
			"types": "./dist/input/Input.svelte.d.ts",
			"svelte": "./dist/input/Input.svelte"
		},
		"./Loading.svelte": {
			"types": "./dist/loading/Loading.svelte.d.ts",
			"svelte": "./dist/loading/Loading.svelte"
		},
		"./Dot.svelte": {
			"types": "./dist/dot/Dot.svelte.d.ts",
			"svelte": "./dist/dot/Dot.svelte"
		},
		"./ChevronIcon.svelte": {
			"types": "./dist/icons/ChevronIcon.svelte.d.ts",
			"svelte": "./dist/icons/ChevronIcon.svelte"
		},
		"./CheckMarkIcon.svelte": {
			"types": "./dist/icons/CheckMarkIcon.svelte.d.ts",
			"svelte": "./dist/icons/CheckMarkIcon.svelte"
		},
		"./Checkbox.svelte": {
			"types": "./dist/checkbox/Checkbox.svelte.d.ts",
			"svelte": "./dist/checkbox/Checkbox.svelte"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@cspell/eslint-plugin": "^8.1.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
